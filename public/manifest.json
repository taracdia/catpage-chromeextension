{
  "content_security_policy": "script-src 'self' 'sha256-Fk630K89GEbmIwmi3sgMaf3tYbfMdF0e616h2Bslv9g='; object-src 'self'",
  "manifest_version": 2,
  "name": "Cat Mewseum",
  "description": "Get cats in your new tab! See your favorites, or random cats!",
  "version": "2.0",
  "author": "Tara Dia",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "Cat Mewseum"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [ 
    "activeTab",
    "identity",
    "storage"
  ],
  "optional_permissions": [ 
    "identity.email"
  ]
}