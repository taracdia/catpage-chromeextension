{
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'",
  "manifest_version": 2,
  "name": "Cat Mewseum",
  "description": "Get cats in your new tab! See your favorites, or random cats!",
  "version": "2.0",
  "author": "Tara Dia",
  "browser_action": {
    "default_icon": "./public/tab-icon.png",
    "default_title": "Cat Mewseum"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": ["./public/background.js"],
    "persistent": false
  },
  "permissions": [ 
    "activeTab",
    "identity",
    "identity.email",
    "storage"
  ]
}